.container.breadcrumbs
  =link_to "Services", admin_services_path
  %span.arrow >
  Edit service
.container.white_bg
  %h2
    Edit
    = @service.name

  = simple_form_for [:admin, @service] do |f|

    = render 'shared/services/form_fields', service: @service, f: f

    = f.button :submit, "Publish this service", class: "button left"
    %br
